{
  "manifest_version": 2,
  "name": "Blendle Related Articles Plugin",
  "version": "1.0.0",

  "content_scripts": [
    {
      "matches": ["http://*./*"],
      "css": ["style.css"],
      "js": ["main.js"]
    }
  ],

  "permissions": [
    "tabs", "http://*./*"
  ],

  "minimum_chrome_version": "23"
}